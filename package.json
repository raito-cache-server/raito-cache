{
  "name": "raito-cache",
  "version": "1.1.0",
  "description": "Lite caching proxy server",
  "main": "dist/bin/index.js",
  "bin": {
    "cache-proxy": "dist/bin/index.js",
    "raito": "dist/bin/index.js"
  },
  "repository": "https://github.com/stbestichhh/raito-cache.git",
  "author": "Kiril Yakymchuk <stbestich@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "caching",
    "cache",
    "caching-proxy",
    "cli"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npx tsc",
    "start": "tsx bin/index.ts",
    "start:dev": "tsx watch bin/index.ts",
    "start:prod": "node dist/bin/index.js",
    "lint": "eslint .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "hono": "^4.6.12",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/js": "^9.15.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.3",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^8.57.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  }
}
